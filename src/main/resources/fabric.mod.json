{
  "schemaVersion": 1,
  "id": "packed",
  "version": "${version}",
  "name": "Packed Storage",
  "description": "Packed is a mod that adds a variety of storage blocks such as chest, barrels, and crates. These storage blocks all have different tiers, with each tier the blocks has the ability to store more items than the previous one.",
  "authors": [
    "manymoney2",
    "Louis9902"
  ],
  "contact": {
    "issues": "https://github.com/minnecraeft/packed/issues",
    "website": "https://www.curseforge.com/minecraft/mc-mods/packed-storage"
  },
  "license": "GPL-3.0",
  "icon": "assets/packed/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "de.geekeey.packed.PackedClient"
    ],
    "main": [
      "de.geekeey.packed.Packed"
    ]
  },
  "mixins": [
    "packed.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.9.0+build.204",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "accessWidener": "packed.accesswidener"
}